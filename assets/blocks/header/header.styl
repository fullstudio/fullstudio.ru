header
  .color-overlay
    min-height 900px
  .full-screen
    width 100%
    height 100px
  .only-logo
    .navbar
      background none
      padding 60px
      .navbar-header
        float none
  .home-contents
    margin-top 5%
  .intro-section
    text-align left
    .standard-button
      margin 0
    .intro
      color #ffffff
      margin-bottom 36px
    h5
      color #ffffff
    .buttons
      margin-top 35px
  .phone-image
    max-width 350px
    margin auto
    margin-bottom 50px
    img
      width 100%

.sticky-navigation
  min-height 60px
  background #111111
  border none
  padding 0 !important
  margin-bottom 0 !important
  -webkit-backface-visibility hidden
  -webkit-box-shadow 0px 2px 8px 0px rgba(50, 50, 50, 0.08)
  box-shadow 0px 2px 8px 0px rgba(50, 50, 50, 0.08)
  .navbar-header
    img
      max-height 30px
      -webkit-backface-visibility hidden
  .main-navigation
    .current
      a
        color #FFFFFF
        position relative
        outline none
    li
      a
        line-height 30px
        font-weight 400
        font-size 13px
        text-transform uppercase
        color #c8c8c8
